package swingapp;

import java.awt.Color;

import javax.swing.*;

public class DefaultPanel {
	private JPanel panel;

    public DefaultPanel() {
        panel = new JPanel();
        JLabel label = new JLabel("Quản lý ngôn ngữ lập trình!");
        label.setSize(50, 50);
        panel.add(label);
        panel.setBackground(Color.LIGHT_GRAY);
    }

    public JPanel getPanel() {
        return panel;
    }
}
